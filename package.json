{
  "name": "tripfluence-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "dev:queue": "node jobs/worker.ts",
    "dev:all": "concurrently -k -n web,queue \"next dev --port 3000\" \"npm run dev:queue\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:update": "playwright test --update-snapshots",
    "test:align": "playwright test tests/e2e/alignment.spec.ts",
    "test:visual": "playwright test tests/e2e/visual-baseline.spec.ts",
    "test:a11y": "playwright test tests/e2e/a11y.spec.ts",
    "test:perf": "playwright test tests/e2e/perf.spec.ts",
    "test:responsive": "playwright test tests/e2e/responsive-audit.spec.ts",
    "test:dead-clicks": "playwright test tests/e2e/dead-click-audit.spec.ts",
    "build:prod": "next build",
    "start:prod": "next start -p 4000",
    "e2e:prod": "start-server-and-test start:prod http://localhost:4000 'playwright test --project=desktop-chromium'",
    "test:deadclicks": "playwright test tests/e2e/deadclicks.spec.ts",
    "deadlinks": "linkinator http://localhost:3000 --recurse --silent --format json > reports/deadlinks.json",
    "deadlinks:report": "playwright test tests/e2e/deadlink-audit.spec.ts",
    "test:unit": "vitest --run",
    "test:api": "vitest --run tests/api",
    "test:integration": "vitest --run tests/integration",
    "test:all": "npm run test:unit && npm run test:api && npm run test:integration",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.87.1",
    "@tanstack/react-table": "^8.21.3",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.58.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "geist": "^1.5.1",
    "ioredis": "^5.7.0",
    "lucide-react": "^0.542.0",
    "nanoid": "^5.1.5",
    "next": "^14.2.5",
    "next-themes": "^0.4.6",
    "pino": "^9.9.4",
    "pino-pretty": "^13.1.1",
    "prisma": "^6.15.0",
    "react": "^18.3.1",
    "react-day-picker": "^9.9.0",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.19.13",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^9.2.1",
    "eslint": "^8",
    "eslint-config-next": "^14.2.5",
    "jsdom": "^27.0.0",
    "lighthouse": "^12.8.2",
    "linkinator": "^6.1.4",
    "node-mocks-http": "^1.17.2",
    "playwright-lighthouse": "^4.0.0",
    "start-server-and-test": "^2.1.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
